<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns   = "http://www.w3.org/ns/widgets"
 xmlns:gap   = "http://phonegap.com/ns/1.0"
 id          = "edu.uab.business.studentapp"
 versionCode = "1"
 version     = "0.0.1" >


<!--versionCode is optional and Android only -->

<name>Andrea Birchfield Assignment 5</name>

<description>
    Andrea Birchfield Assignment 5

</description>

<author>
    Andrea L Birchfield
</author>

<preference name="phonegap-version" value="cli-6.5.0" />
<plugin name="cordova-plugin-whitelist" source="npm" />
<access origin="*" />


<plugin name="cordova-plugin-geolocation" spec="2.4.3" />
<plugin name="cordova-plugin-splashscreen" source="npm" />



    
    <preference name="SplashScreen" value="splash"></preference>
    <splash src = "splash.png" />
    <!--<splash src="splash.png" width="320" height="480"/>-->
   <!--<splash src="res/screen/ios/Default@2x~universal~splash.png" />-->
   
  

<preference name="SplashScreenDelay" value="10000" />

<!--<icon src = "icon.png"/>-->

 
 <platform name = "ios">
    <icon src="uab.png" platform="ios" width="57" height="57" density="mdpi" />
    
 </platform>
 <platform name="android">
        <!--
            ldpi    : 36x36 px
            mdpi    : 48x48 px
            hdpi    : 72x72 px
            xhdpi   : 96x96 px
            xxhdpi  : 144x144 px
            xxxhdpi : 192x192 px
        -->
        <icon src="icon.png" density="ldpi" />
        
    </platform>
  

     <platform name="windows">
        <icon src="icon.png" target="StoreLogo" />
        
    </platform>
     
</widget>